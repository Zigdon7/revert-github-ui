{
  "name": "Revert GitHub UI",
  "description": "Make Github.com's UI great again.",
  "manifest_version": 2,
  "version": "1.0",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*"
      ],
      "js": ["index.js"]
    }
  ],
  "permissions": ["tabs"]
}
